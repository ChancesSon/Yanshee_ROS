<robot name="right_leg4" xmlns:xacro="http://wiki.ros.org/xacro">
  <xacro:property name="part_name" value="right_leg4" />
  <xacro:property name="origin_x" value="0.0339764646323713" />
  <xacro:property name="origin_y" value="0.000681371450579888" />
  <xacro:property name="origin_z" value="0.00157592285558581" />
  <xacro:property name="mass_value" value="0.0377471105564984" />

  
  <xacro:property name="inertia_ixx" value="9.39595739028984E-06" />
  <xacro:property name="inertia_ixy" value="-6.93417906435305E-08" />
  <xacro:property name="inertia_ixz" value="-1.98734856222991E-07" />
  <xacro:property name="inertia_iyy" value="9.74975907787965E-06" />
  <xacro:property name="inertia_iyz" value="2.15816104840149E-07" />
  <xacro:property name="inertia_izz" value="6.29758000243913E-06" /> 

  <xacro:property name="mesh_file" value="package://yanshee/meshes/right_leg4_Link.STL" />

  <xacro:property name="joint_offset_x" value="0.059245" />
  <xacro:property name="joint_offset_y" value="-1.5243E-05" />
  <xacro:property name="joint_offset_z" value="-1.0003E-05" />
  <xacro:property name="joint_offset_rr" value="0" />
  <xacro:property name="joint_offset_rp" value="0" />
  <xacro:property name="joint_offset_ry" value="0" />

  <xacro:property name="parent_link" value="right_leg3_Link" />
  <xacro:property name="child_link" value="right_leg4_Link" />

  <xacro:property name="joint_axis_x" value="0" />
  <xacro:property name="joint_axis_y" value="0" />
  <xacro:property name="joint_axis_z" value="-1" />

  <xacro:property name="joint_limit_lower" value="-1.58" />
  <xacro:property name="joint_limit_upper" value="1.58" />
  <xacro:property name="joint_limit_effort" value="1" />
  <xacro:property name="joint_limit_velocity" value="10" />





  <link name="${part_name}_Link">
    <inertial>
      <origin xyz="${origin_x} ${origin_y} ${origin_z}" rpy="0 0 0" />
      <mass value="${mass_value}" />
      <inertia
        ixx="${inertia_ixx}"
        ixy="${inertia_ixy}"
        ixz="${inertia_ixz}"
        iyy="${inertia_iyy}"
        iyz="${inertia_iyz}"
        izz="${inertia_izz}" />
    </inertial>

    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="${mesh_file}" />
      </geometry>
      <material name="">
        <color rgba="1 1 1 1" />
      </material>
    </visual>

    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="${mesh_file}" />
      </geometry>
    </collision>
  </link>

  <gazebo reference="${part_name}">
  </gazebo>

  <joint name="${part_name}_joint" type="revolute">
    <origin xyz="${joint_offset_x} ${joint_offset_y} ${joint_offset_z}" rpy="${joint_offset_rr} ${joint_offset_rp} ${joint_offset_ry}" />
    <parent link="${parent_link}" />
    <child link="${child_link}" />
    <axis xyz="${joint_axis_x} ${joint_axis_y} ${joint_axis_z}" />
    <limit lower="${joint_limit_lower}" upper="${joint_limit_upper}" effort="${joint_limit_effort}" velocity="${joint_limit_velocity}" />
  </joint>

  <gazebo reference="${part_name}_joint">
    <implicitSpringDamper>true</implicitSpringDamper>
  </gazebo>

</robot>
